{
  "name": "Cyber-Dexter-MD-Bot",
  "description": "A powerful WhatsApp Multi-Device Bot made by CYBER DEXTER",
  "repository": "https://github.com/CYBER-DEXTER-MD-V1/CYBER-DEXTER-MD",
  "keywords": [
    "whatsapp",
    "bot",
    "nodejs",
    "baileys",
    "multi-device",
    "cyber-dexter"
  ],
  "env": {
    "SESSION_ID": {
      "description": "Your Session ID",
      "required": true
    },
    "PREFIX": {
      "description": "Bot command prefix (default is .)",
      "required": false,
      "value": "."
    },
    "GEMINI_KEY": {
      "description": "Google Gemini API Key for AI features",
      "required": true
    }
  },
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ]
}
